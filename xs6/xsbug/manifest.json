{	"build":{		"F_EXTENSIONS":"$(F_HOME)/extensions",		"F_KINOMA":"$(F_HOME)/kinoma",		"KDT_HOME":"$(F_KINOMA)/kpr/projects/kdt",		"KPL_HOME":"$(F_HOME)/build/linux/kpl",		"KPR_HOME":"$(F_KINOMA)/kpr",		"XSBUG_HOME":"$(F_HOME)/xs6/xsbug",	},	"environment":{		"VERSION":"1.0",		"NAME":"xsbug",		"NAMESPACE":"com.marvell.kinoma.shell.xsbug",		"CA_list":"ca-bundle.crt",				"modulePath":"[applicationPath]modules/",		"shellPath":"[applicationPath]program/xsbug.xsb",				"httpServerPort":"10000",		"useSSDP":"1",		"ssdpExpire":"1800",		"ssdpSearchAll":"0",		"ssdpTTL":"4",		"ssdpByebyeRepeat":"3",		"screenScale":"1",		"windowStyle":"0",				"FSK":"$(F_HOME)",		"XS":"$(F_HOME)/xs",		"XS6":"$(F_HOME)/xs6",		"KPR":"$(KPR_HOME)",		"KDT":"$(KDT_HOME)",		"debugger":"192.168.0.8:5002",		"useGL":"1",	},	"extensions":{		"fsZip":"$(F_EXTENSIONS)/fsZip/fsZip.mk",		"Crypt":"$(F_EXTENSIONS)/crypt/Crypt.mk",		"FskSSLAll":"$(F_EXTENSIONS)/ssl/FskSSLAll.mk",		"FskBMPCodec":"$(F_EXTENSIONS)/FskBMPCodec/FskBMPCodec.mk",		"FskGIFDecode":"$(F_EXTENSIONS)/FskGIFDecode/FskGIFDecode.mk",		"FskJPEGDecode":"$(F_EXTENSIONS)/FskJPEGDecode/FskJPEGDecode.mk",		"FskPNGDecode":"$(F_EXTENSIONS)/FskPNGDecode/FskPNGDecode.mk",		"mediareader":"$(F_KINOMA)/mediareader/mediareader.mk",		"kpr":"$(KPR_HOME)/kpr.mk",		"kprDebug":"$(XSBUG_HOME)/debug/kprDebug.mk",	},	"resources":{		".":[			"$(F_HOME)/data/sslcert/ca-bundle.crt",		],		"modules":[			"$(KPR_HOME)/libraries/MobileFramework/src/*",			"$(XSBUG_HOME)/modules/*",		],		"program": [			"$(XSBUG_HOME)/assets",			"$(XSBUG_HOME)/xsbug.js",		],	},	"platforms":{		"linux":{			"environment":{			},			"fonts":{				"default": "Fira Sans",				"path": "[applicationPath]fonts/"			},			"resources":{				"fonts": [					"$(F_HOME)/data/fonts/FiraMono-Regular.ttf",					"$(F_HOME)/data/fonts/FiraSans-Bold.ttf",					"$(F_HOME)/data/fonts/FiraSans-Regular.ttf",				],			},		},	},	"instrument":{		"log":"",		"syslog":"",		"trace":true,		"threads":true,		"times":true,		"androidlog":false,		"kinds":[					]	},}