{	"xsdebug":{		"enabled":true,	},	"build":{		"F_EXTENSIONS":"$(F_HOME)/extensions",		"F_KINOMA":"$(F_HOME)/kinoma",		"KDT_HOME":"$(F_KINOMA)/kpr/projects/kdt",		"KPL_HOME":"$(F_HOME)/build/linux/kpl",		"KPR_HOME":"$(F_KINOMA)/kpr",		"SHELL_HOME":"$(F_HOME)/xs6/xsedit",	},	"environment":{		"VERSION":"1.0",		"NAME":"xsedit",		"NAMESPACE":"com.marvell.kinoma.shell.xsedit",		"CA_list":"ca-bundle.crt",				"modulePath":"[applicationPath]modules/",		"shellPath":"[applicationPath]modules/shell/main.xsb",				"httpServerPort":"10000",		"useSSDP":"1",		"ssdpExpire":"1800",		"ssdpSearchAll":"0",		"ssdpTTL":"4",		"ssdpByebyeRepeat":"3",		"screenScale":"1",		"windowStyle":"0",		"fitFonts":"0",		"useGL":"1",	},	"extensions":{		"fsZip":"$(F_EXTENSIONS)/fsZip/fsZip.mk",		"Crypt":"$(F_EXTENSIONS)/crypt/Crypt.mk",		"FskSSLAll":"$(F_EXTENSIONS)/ssl/FskSSLAll.mk",		"FskBMPCodec":"$(F_EXTENSIONS)/FskBMPCodec/FskBMPCodec.mk",		"FskGIFDecode":"$(F_EXTENSIONS)/FskGIFDecode/FskGIFDecode.mk",		"FskJPEGDecode":"$(F_EXTENSIONS)/FskJPEGDecode/FskJPEGDecode.mk",		"FskPNGDecode":"$(F_EXTENSIONS)/FskPNGDecode/FskPNGDecode.mk",		"mediareader":"$(F_KINOMA)/mediareader/mediareader.mk",		"kpr":"$(KPR_HOME)/kpr.mk",		"kprCode":"$(SHELL_HOME)/code/kprCode.mk",		"kprDebug":"$(SHELL_HOME)/debug/kprDebug.mk",		"kprMarkdown":"$(SHELL_HOME)/markdown/kprMarkdown.mk",		"kprSSDP":"$(KPR_HOME)/extensions/ssdp/kprSSDP.mk",		"kprZeroconf":"$(KPR_HOME)/extensions/zeroconf/kprZeroconf.mk",		"kprPins":"$(KPR_HOME)/extensions/pins/kprPins.mk",		"websocket":"$(KPR_HOME)/extensions/websocket/kprWebSocket.mk",	},	"resources":{		"~":[			"$(SHELL_HOME)/features/devices/helpers/element/icon.png",		],		".":[			"$(F_HOME)/data/sslcert/ca-bundle.crt",		],		"certs":[			"$(SHELL_HOME)/certs/*",		],		"modules":[			"$(SHELL_HOME)/common",			"$(SHELL_HOME)/features",			"$(SHELL_HOME)/shell",			"$(KPR_HOME)/libraries/Pins/src/*",		],	},	"sources": [		"$(SHELL_HOME)/features/devices/configs/create/helper/main.js",		"$(SHELL_HOME)/features/devices/configs/element/helper/main.js",	],	"platforms":{		"linux":{			"xsprofile":{				"enabled":true,			},			"environment":{			},			"fonts":{				"default": "Fira Sans",				"path": "[applicationPath]fonts/"			},			"resources":{				"fonts": [					"$(F_HOME)/data/fonts/FiraMono-Regular.ttf",					"$(F_HOME)/data/fonts/FiraSans-Bold.ttf",					"$(F_HOME)/data/fonts/FiraSans-Regular.ttf",				],			},		},		"mac":{			"xsprofile":{				"enabled":true,			},			"resources":{				"tools": [					"$(F_HOME)/xs6/bin/mac/release/xsc6",					"$(F_HOME)/xs6/bin/mac/release/xsr6",					"$(F_HOME)/xs6/bin/mac/release/modules/tools.so",					"$(F_HOME)/xs6/bin/mac/release/modules/tools.xsa",				],			},		},	},	"instrument":{		"log":"",		"syslog":"",		"trace":true,		"threads":true,		"times":true,		"androidlog":false,		"kinds":[			{ "type":"KprMessage", "messages":"debug" },			{ "type":"KprHTTPConnection", "messages":"debug" },			{ "type":"KprHTTPClient", "messages":"debug" },			{ "type":"KprHTTPTarget", "messages":"debug" },			{ "type":"httpsystem", "messages":"debug" },			{ "type":"httpclient", "messages":"debug" },			{ "type":"httpclientrequest", "messages":"debug" },			{ "type":"httpclientauth", "messages":"debug" },		]	},}