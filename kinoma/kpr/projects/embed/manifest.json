{
	"xsdebug":{
		"enabled":true,
	},
	"xsprofile":{
		"enabled":true,
	},
	"build":{
		"F_EXTENSIONS":"$(F_HOME)/extensions",
		"F_KINOMA":"$(F_HOME)/kinoma",
		"F_MEDIA_READER":"$(F_KINOMA)/mediareader",
		"KDT_HOME":"$(F_KINOMA)/kpr/projects/kdt",
		"KPL_HOME":"$(F_HOME)/build/linux/kpl",
		"KPR_HOME":"$(F_KINOMA)/kpr",
		"SHELL_HOME":"$(F_KINOMA)/kpr/projects/embed",
	},
	"environment":{
		"VERSION":"1.0",
		"NAME":"EmbedShell",
		"NAMESPACE":"com.marvell.kinoma.shell.embed",
		"CA_list":"ca-bundle.crt",
		"httpServerPort":"10002",
		"useSSDP":"1",
		"ssdpExpire":"1800",
		"ssdpSearchAll":"0",
		"ssdpTTL":"4",
		"ssdpByebyeRepeat":"3",
		"hardwarepinsSimulator":"true",
		"modulePath":"[applicationPath]modules/;[applicationPath]modules/pins/",
		"screenScale":"1",
		"httpPoolSize":"5",
		"httpCacheSize":"197",
		"httpCookiesSize":"197",
		"httpKeychainSize":"197",
		"httpLocalStorageSize":"197",
		"textureCacheSize":"2500000",
		"displayFPS":"0",
		"rotateLeftAccel":"[",
		"rotateRightAccel":"]",
		"windowStyle":"0",
		"PICVersion":"11",
		"PICsNeedUpdate":"false",
	},
	"extensions":{
		"fsZip":"$(F_EXTENSIONS)/fsZip/fsZip.mk",
		"Crypt":"$(F_EXTENSIONS)/crypt/Crypt.mk",
		"FskSSLAll":"$(F_EXTENSIONS)/ssl/FskSSLAll.mk",
		"FskBMPCodec":"$(F_EXTENSIONS)/FskBMPCodec/FskBMPCodec.mk",
		"FskGIFDecode":"$(F_EXTENSIONS)/FskGIFDecode/FskGIFDecode.mk",
		"FskJPEGDecode":"$(F_EXTENSIONS)/FskJPEGDecode/FskJPEGDecode.mk",
		"FskJPEGEncode":"$(F_EXTENSIONS)/FskJPEGEncode/FskJPEGEncode.mk",
		"FskPNGDecode":"$(F_EXTENSIONS)/FskPNGDecode/FskPNGDecode.mk",
		"mediareader":"$(F_MEDIA_READER)/mediareader.mk",
		"FskMediaReaderAMR":"$(F_MEDIA_READER)/readers/amr/FskMediaReaderAMR.mk",
		"FskMediaReaderASF":"$(F_MEDIA_READER)/readers/asf/FskMediaReaderASF.mk",
		"FskMediaReaderAVI":"$(F_MEDIA_READER)/readers/avi/FskMediaReaderAVI.mk",
		"FskMediaReaderFLAC":"$(F_MEDIA_READER)/readers/flac/FskMediaReaderFLAC.mk",
		"FskMediaReaderFLV":"$(F_MEDIA_READER)/readers/flv/FskMediaReaderFLV.mk",
		"FskMediaReaderJPEGWebcam":"$(F_MEDIA_READER)/readers/jpeg-webcam/FskMediaReaderJPEGWebcam.mk",
		"FskMediaReaderMP3":"$(F_MEDIA_READER)/readers/mp3/FskMediaReaderMP3.mk",
		"FskMediaReaderMP4":"$(F_MEDIA_READER)/readers/mp4/FskMediaReaderMP4.mk",
		"FskMediaReaderMPEG":"$(F_MEDIA_READER)/readers/mpeg/FskMediaReaderMPEG.mk",
		"FskMediaReaderRTSP":"$(F_MEDIA_READER)/readers/rtsp/FskMediaReaderRTSP.mk",
		"FskMediaReaderWave":"$(F_MEDIA_READER)/readers/wave/FskMediaReaderWave.mk",
		"FskMediaReaderYUV420":"$(F_MEDIA_READER)/readers/yuv420/FskMediaReaderYUV420.mk",
		"flvmuxer":"$(F_KINOMA)/flvmuxer/flvmuxer.mk",
		"kinomayuv420dec":"$(F_KINOMA)/kinoma-yuv420-dec/kinomayuv420dec.mk",
		"kpr":"$(KPR_HOME)/kpr.mk",
		"kprPins":"$(KPR_HOME)/extensions/pins/kprPins.mk",
		"kprSSDP":"$(KPR_HOME)/extensions/ssdp/kprSSDP.mk",
		"kprUPnP":"$(KPR_HOME)/extensions/upnp/kprUPnP.mk",
		"kprWiFi":"$(KPR_HOME)/extensions/wifi/kprWiFi.mk",
		"kprWebSocket":"$(KPR_HOME)/extensions/websocket/kprWebSocket.mk",
		"kprZeroconf":"$(KPR_HOME)/extensions/zeroconf/kprZeroconf.mk",
		"kprCoAP":"$(KPR_HOME)/extensions/coap/kprCoAP.mk",
		"kprMQTT":"$(KPR_HOME)/extensions/mqtt/kprMQTT.mk",
		"kprXMPP":"$(KPR_HOME)/extensions/xmpp/kprXMPP.mk",
	},
	"fonts":{
		"default": "Fira Sans",
		"path": "[applicationPath]fonts/"
	},
	"resources":{
		"~": [
		],
		".":[
			"$(F_HOME)/data/sslcert/ca-bundle.crt"
		],
		"modules":[
			"$(KPR_HOME)/libraries/MobileFramework/src/*",
			"$(KPR_HOME)/libraries/Transitions/src/*",
			"$(KPR_HOME)/libraries/Controls/src/*",
			"$(KPR_HOME)/libraries/Localization/src/*",
			"$(KPR_HOME)/libraries/Creations/src/*",
			"$(KPR_HOME)/libraries/MultiTouchLib/src/*",
			"$(KPR_HOME)/libraries/Pins/src/*",
			"$(KPR_HOME)/libraries/LowPAN/src/*",
			"$(SHELL_HOME)/common/*",
		],
		"fonts": [
			"$(F_HOME)/data/fonts/FiraMono-Regular.ttf",
			"$(F_HOME)/data/fonts/FiraSans-Bold.ttf",
			"$(F_HOME)/data/fonts/FiraSans-Regular.ttf",
		],
	},
	"platforms":{
		"linux":{
			"environment":{
				"fontsPath":"[applicationPath]fonts/",
				"ssdpDiscoverSelf":"0",
			},
			"extensions":{
				"FskMediaReaderAMR":null,
				"FskMediaReaderASF":null,
				"FskMediaReaderAVI":null,
				"FskMediaReaderFLAC":null,
				"FskMediaReaderJPEGWebcam":null,
				"FskMediaReaderMPEG":null,
				"FskMediaReaderPCM":"$(F_MEDIA_READER)/readers/pcm/FskMediaReaderPCM.mk",
				"FskMediaReaderRTSP":null,
				"FskMediaReaderYUV420":null,
				"flvmuxer":null,
				"kinomayuv420dec":null,
				"FskPinDigitalLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalLinux/FskPinDigitalLinux.mk",
				"FskPinI2CLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CLinux/FskPinI2CLinux.mk",
				"FskPinSerialLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialLinux/FskPinSerialLinux.mk",
				"kprUPnP":null,
			},
			"platforms":{
				"gtk":{
					"extensions":{
					},
					"build":{
						"SIMULATOR":true,
					},
				},
				"beaglebone":{
					"build":{
						"DEVICE":true,
						"DEVICE_HOME":"$(SHELL_HOME)/devices/beaglebone",
					},
					"environment":{
						"KPR_BINARY_NAME":"EmbedShell",
						"useGL":"0",
					},
					"extensions":{
						"KplFrameBuffer":"$(KPL_HOME)/framebuffer/KplFrameBuffer.mk",
						"FskPinDigitalMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalMRAA/FskPinDigitalMRAA.mk",
						"FskPinAnalogMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogMRAA/FskPinAnalogMRAA.mk",
						"FskPinI2CMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CMRAA/FskPinI2CMRAA.mk",
						"FskPinPWMMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMMRAA/FskPinPWMMRAA.mk",
						"FskPinSerialMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialMRAA/FskPinSerialMRAA.mk",

					},
				},
				"edison":{
					"build":{
						"DEVICE":true,
						"DEVICE_HOME":"$(SHELL_HOME)/devices/edison",
					},
					"environment":{
						"KPR_BINARY_NAME":"EmbedShell",
						"useGL":"0",
					},
					"extensions":{
						"KplFrameBuffer":"$(KPL_HOME)/framebuffer/KplFrameBuffer.mk",
						"FskPinDigitalMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalMRAA/FskPinDigitalMRAA.mk",
						"FskPinAnalogMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogMRAA/FskPinAnalogMRAA.mk",
						"FskPinI2CMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CMRAA/FskPinI2CMRAA.mk",
						"FskPinPWMMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMMRAA/FskPinPWMMRAA.mk",
						"FskPinSerialMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialMRAA/FskPinSerialMRAA.mk",
					},
				},
				"pi_gl":{
					"build":{
						"DEVICE":true,
						"DEVICE_HOME":"$(SHELL_HOME)/devices/pi",
					},
					"environment":{
						"KPR_BINARY_NAME":"EmbedShell",
						"useGL":"1",
					},
					"extensions":{
						"KplFrameBuffer":"$(KPL_HOME)/framebuffer/KplFrameBuffer.mk",
						"FskPinDigitalMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalMRAA/FskPinDigitalMRAA.mk",
						"FskPinAnalogMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogMRAA/FskPinAnalogMRAA.mk",
						"FskPinI2CMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CMRAA/FskPinI2CMRAA.mk",
						"FskPinPWMMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMMRAA/FskPinPWMMRAA.mk",
						"FskPinSerialMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialMRAA/FskPinSerialMRAA.mk",
					},
				},
				"pi":{
					"build":{
						"DEVICE":true,
						"DEVICE_HOME":"$(SHELL_HOME)/devices/pi",
					},
					"environment":{
						"KPR_BINARY_NAME":"EmbedShell",
						"useGL":"0",
					},
					"extensions":{
						"KplFrameBuffer":"$(KPL_HOME)/framebuffer/KplFrameBuffer.mk",
						"FskPinDigitalMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalMRAA/FskPinDigitalMRAA.mk",
						"FskPinAnalogMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogMRAA/FskPinAnalogMRAA.mk",
						"FskPinI2CMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CMRAA/FskPinI2CMRAA.mk",
						"FskPinPWMMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMMRAA/FskPinPWMMRAA.mk",
						"FskPinSerialMRAA":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialMRAA/FskPinSerialMRAA.mk",
					},
				},
			}
		},
		"mac":{
			"build":{
				"SIMULATOR":true,
			},
			"environment":{
				"touchSupported":"true",
			},
			"extensions":{
				"FskCameraAVF":null,
				"kinomaqtdec":"$(F_KINOMA)/kinoma-qt-dec/kinomaqtdec.mk",
				"kprLibrary":null,
			},
		},
		"win":{
			"build":{
				"SIMULATOR":true,
			},
			"environment":{
				"fontsPath":"[applicationPath]fonts/",
			},
			"extensions":{
				"FskCameraDshow":"$(F_MEDIA_READER)/readers/camera-dshow/FskCameraDshow.mk",
				"FskDMOCodec":"$(F_KINOMA)/DMOCodec/FskDMOCodec.mk",
				"FskMediaReaderFLAC":null,
			},
			"resources":{
				"~": [

				]
			},
		},
	},
	"variants":{
		"$(DEVICE)": {
			"environment":{
				"hwpModulePath":"device",
				"shellPath":"[applicationPath]modules/shell.xsb",
			},
			"resources":{
				"~":[
				],
				"modules":[
					"$(DEVICE_HOME)/*",
				],
			},
		},
		"$(SIMULATOR)": {
			"environment":{
				"hwpModulePath":"simulator",
				"shellPath":"[applicationPath]modules/shell.xsb",
				"useGL":"1",
			},
			"resources":{
				"~":[
					"$(SHELL_HOME)/devices/beaglebone/pins",
					"$(SHELL_HOME)/devices/beaglebone/shell.js",
					"$(SHELL_HOME)/devices/beaglebone/system",
					"$(SHELL_HOME)/devices/edison/pins",
					"$(SHELL_HOME)/devices/edison/shell.js",
					"$(SHELL_HOME)/devices/edison/system",
					"$(SHELL_HOME)/devices/pi/pins",
					"$(SHELL_HOME)/devices/pi/shell.js",
					"$(SHELL_HOME)/devices/pi/system",
				],
				".":[
				],
				"modules":[
					"$(KDT_HOME)/kpr/simulator/modules/assets",
					"$(KDT_HOME)/kpr/simulator/modules/PinsSimulators.xml",
					"$(SHELL_HOME)/devices",
					"$(SHELL_HOME)/simulator/*",
				],
			},
		},
	},
	"instrument":{
		"log":"",
		"syslog":"",
		"trace":false,
		"threads":false,
		"times":false,
		"androidlog":false,
		"kinds":[

		]
	},
}
