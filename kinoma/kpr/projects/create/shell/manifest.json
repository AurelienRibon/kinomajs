{
	"xsdebug":{
		"enabled":true,
	},
	"xsprofile":{
		"enabled":true,
	},
	"build":{
		"F_EXTENSIONS":"$(F_HOME)/extensions",
		"F_KINOMA":"$(F_HOME)/kinoma",
		"F_MEDIA_READER":"$(F_KINOMA)/mediareader",
		"KDT_HOME":"$(F_KINOMA)/kpr/projects/kdt",
		"KPL_HOME":"$(F_HOME)/build/linux/kpl",
		"KPR_HOME":"$(F_KINOMA)/kpr",
		"CREATE_HOME":"$(F_KINOMA)/kpr/projects/create",
	},
	"environment":{
		"VERSION":"2.0",
		"NAME":"CreateShell",
		"NAMESPACE":"com.marvell.kinoma.shell.create",
		"CA_list":"ca-bundle.crt",
		"httpServerPort":"10000",
		"useSSDP":"1",
		"ssdpExpire":"1800",
		"ssdpSearchAll":"0",
		"ssdpTTL":"4",
		"ssdpByebyeRepeat":"3",
		"hardwarepinsSimulator":"true",
		"modulePath":"[applicationPath]modules/",
		"screenScale":"1",
		"httpPoolSize":"5",
		"httpCacheSize":"197",
		"httpCookiesSize":"197",
		"httpKeychainSize":"197",
		"httpLocalStorageSize":"197",
		"textureCacheSize":"2500000",
		"displayFPS":"0",
		"rotateLeftAccel":"[",
		"rotateRightAccel":"]",
		"windowStyle":"0",
		"PICVersion":"11",
		"PICsNeedUpdate":"false",
	},
	"extensions":{
		"fsZip":"$(F_EXTENSIONS)/fsZip/fsZip.mk",
		"Crypt":"$(F_EXTENSIONS)/crypt/Crypt.mk",
		"FskSSLAll":"$(F_EXTENSIONS)/ssl/FskSSLAll.mk",
		"FskBMPCodec":"$(F_EXTENSIONS)/FskBMPCodec/FskBMPCodec.mk",
		"FskGIFDecode":"$(F_EXTENSIONS)/FskGIFDecode/FskGIFDecode.mk",
		"FskJPEGDecode":"$(F_EXTENSIONS)/FskJPEGDecode/FskJPEGDecode.mk",
		"FskJPEGEncode":"$(F_EXTENSIONS)/FskJPEGEncode/FskJPEGEncode.mk",
		"FskPNGDecode":"$(F_EXTENSIONS)/FskPNGDecode/FskPNGDecode.mk",
		"mediareader":"$(F_MEDIA_READER)/mediareader.mk",
		"FskMediaReaderAMR":"$(F_MEDIA_READER)/readers/amr/FskMediaReaderAMR.mk",
		"FskMediaReaderASF":"$(F_MEDIA_READER)/readers/asf/FskMediaReaderASF.mk",
		"FskMediaReaderAVI":"$(F_MEDIA_READER)/readers/avi/FskMediaReaderAVI.mk",
		"FskMediaReaderFLAC":"$(F_MEDIA_READER)/readers/flac/FskMediaReaderFLAC.mk",
		"FskMediaReaderFLV":"$(F_MEDIA_READER)/readers/flv/FskMediaReaderFLV.mk",
		"FskMediaReaderJPEGWebcam":"$(F_MEDIA_READER)/readers/jpeg-webcam/FskMediaReaderJPEGWebcam.mk",
		"FskMediaReaderMP3":"$(F_MEDIA_READER)/readers/mp3/FskMediaReaderMP3.mk",
		"FskMediaReaderMP4":"$(F_MEDIA_READER)/readers/mp4/FskMediaReaderMP4.mk",
		"FskMediaReaderMPEG":"$(F_MEDIA_READER)/readers/mpeg/FskMediaReaderMPEG.mk",
		"FskMediaReaderRTSP":"$(F_MEDIA_READER)/readers/rtsp/FskMediaReaderRTSP.mk",
		"FskMediaReaderWave":"$(F_MEDIA_READER)/readers/wave/FskMediaReaderWave.mk",
		"FskMediaReaderYUV420":"$(F_MEDIA_READER)/readers/yuv420/FskMediaReaderYUV420.mk",
		"flvmuxer":"$(F_KINOMA)/flvmuxer/flvmuxer.mk",
		"kinomaaacipp":"$(F_KINOMA)/kinoma-aac-ipp/kinomaaacipp.mk",
		"kinomaavc":"$(F_KINOMA)/kinoma-avc/kinomaavc.mk",
		"kinomamp3ipp":"$(F_KINOMA)/kinoma-mp3-ipp/kinomamp3ipp.mk",
		"kinomamp4ipp":"$(F_KINOMA)/kinoma-mp4-ipp/kinomamp4ipp.mk",
		"kinomaspeexdec":"$(F_KINOMA)/kinoma-speex/kinomaspeexdec.mk",
		"kinomaspeexenc":"$(F_KINOMA)/kinoma-speex/kinomaspeexenc.mk",
		"kinomayuv420dec":"$(F_KINOMA)/kinoma-yuv420-dec/kinomayuv420dec.mk",
		"create":"$(KDT_HOME)/create/extensions/create/create.mk",
		"kpr":"$(KPR_HOME)/kpr.mk",
		"kprPins":"$(KPR_HOME)/extensions/pins/kprPins.mk",
		"kprSSDP":"$(KPR_HOME)/extensions/ssdp/kprSSDP.mk",
		"kprUPnP":"$(KPR_HOME)/extensions/upnp/kprUPnP.mk",
		"kprWiFi":"$(KPR_HOME)/extensions/wifi/kprWiFi.mk",
		"kprWebSocket":"$(KPR_HOME)/extensions/websocket/kprWebSocket.mk",
		"kprZeroconf":"$(KPR_HOME)/extensions/zeroconf/kprZeroconf.mk",
		"kprK4":"$(KPR_HOME)/extensions/k4/kprK4.mk",
		"kprTouchd":"$(KPR_HOME)/extensions/touchd/kprTouchd.mk",
		"kprCoAP":"$(KPR_HOME)/extensions/coap/kprCoAP.mk",
		"kprMQTT":"$(KPR_HOME)/extensions/mqtt/kprMQTT.mk",
		"kprXMPP":"$(KPR_HOME)/extensions/xmpp/kprXMPP.mk",
	},
	"fonts":{
		"default": "Fira Sans",
		"path": "[applicationPath]fonts/"
	},
	"resources":{
		"~": [
			"$(CREATE_HOME)/shell/src/eula.xml",
			"$(CREATE_HOME)/shell/src/NetworkHandlers.xml",
			"$(CREATE_HOME)/shell/src/PinmuxAlert.xml",
			"$(CREATE_HOME)/shell/src/SettingsHandlers.xml",
			"$(CREATE_HOME)/shell/src/ShellContainer.xml",
			"$(CREATE_HOME)/apps/pindetective/src/explorerCommon.xml",
			"$(CREATE_HOME)/apps/pindetective/src/hexInputDials.xml",
			"$(CREATE_HOME)/apps/pindetective/src/inspectors.xml",
			"$(CREATE_HOME)/apps/pindetective/src/settings.xml",
			"$(CREATE_HOME)/apps/pinmux/src/pinmux.xml",
			"$(CREATE_HOME)/apps/settings/src/about.xml",
			"$(CREATE_HOME)/apps/settings/src/update.xml",
			"$(CREATE_HOME)/apps/settings/src/sdcard.xml"
		],
		".":[
			"$(F_HOME)/data/sslcert/ca-bundle.crt"
		],
		"apps":[
			"$(CREATE_HOME)/apps/battery",
			"$(CREATE_HOME)/apps/files",
			"$(CREATE_HOME)/apps/filesharing",
			"$(CREATE_HOME)/apps/logs",
			"$(CREATE_HOME)/apps/netscanner",
			"$(CREATE_HOME)/apps/pindetective",
			"$(CREATE_HOME)/apps/pinmux",
			"$(CREATE_HOME)/apps/samples",
			"$(CREATE_HOME)/apps/settings",
			"$(CREATE_HOME)/apps/wifi",
			"$(CREATE_HOME)/apps/ble-explorer",
		],
		"modules":[
			"$(KDT_HOME)/kpr/core/modules/*",
			"$(KPR_HOME)/libraries/MobileFramework/src/*",
			"$(KPR_HOME)/libraries/Transitions/src/*",
			"$(KPR_HOME)/libraries/Controls/src/*",
			"$(KPR_HOME)/libraries/Localization/src/*",
			"$(KPR_HOME)/libraries/Creations/src/*",
			"$(KPR_HOME)/libraries/MultiTouchLib/src/*",
			"$(KPR_HOME)/libraries/Pins/src/*",
			"$(KPR_HOME)/libraries/LowPAN/src/*"
		],
		"program":[
			"$(CREATE_HOME)/shell/src",
			"$(CREATE_HOME)/shell/upnp",
			"$(CREATE_HOME)/shell/icon.png"
		],
		"fonts": [
			"$(F_HOME)/data/fonts/FiraMono-Regular.ttf",
			"$(F_HOME)/data/fonts/FiraSans-Bold.ttf",
			"$(F_HOME)/data/fonts/FiraSans-Regular.ttf",
		],
	},
	"platforms":{
		"linux":{
			"environment":{
				"fontsPath":"[applicationPath]fonts/",
				"ssdpDiscoverSelf":"0",
			},
			"extensions":{
				"FskMediaReaderAMR":null,
				"FskMediaReaderASF":null,
				"FskMediaReaderAVI":null,
				"FskMediaReaderFLAC":null,
				"FskMediaReaderJPEGWebcam":null,
				"FskMediaReaderMPEG":null,
				"FskMediaReaderPCM":"$(F_MEDIA_READER)/readers/pcm/FskMediaReaderPCM.mk",
				"FskMediaReaderRTSP":null,
				"FskMediaReaderYUV420":null,
				"flvmuxer":null,
				"kinomaaacipp":null,
				"kinomaavc":null,
				"kinomamp3ipp":null,
				"kinomamp4ipp":null,
				"kinomaspeexdec":null,
				"kinomaspeexenc":null,
				"kinomayuv420dec":null,
				"coreaudiofilters":null,
				"kinomaaacdecipp":"$(F_KINOMA)/kinoma-aac-dec-ipp/kinomaaacdecipp.mk",
				"kinomaavcdecipp":"$(F_KINOMA)/kinoma-avc-dec-ipp/kinomaavcdecipp.mk",
				"kinomamp3decipp":"$(F_KINOMA)/kinoma-mp3-dec-ipp/kinomamp3decipp.mk",
				"kinomamp4decipp":"$(F_KINOMA)/kinoma-mp4-dec-ipp/kinomamp4decipp.mk",
				"kinomapngdecipp":"$(F_KINOMA)/kinoma-png-dec-ipp/kinomapngdecipp.mk",
				"FskPinDigitalLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalLinux/FskPinDigitalLinux.mk",
				"FskPinI2CLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CLinux/FskPinI2CLinux.mk",
				"FskPinSerialLinux":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialLinux/FskPinSerialLinux.mk",
				"kprUPnP":null,
			},
			"platforms":{
				"gtk":{
					"extensions":{
						"kinomaaacipp":"$(F_KINOMA)/kinoma-aac-ipp/kinomaaacipp.mk",
						"kinomaavc":"$(F_KINOMA)/kinoma-avc/kinomaavc.mk",
						"kinomamp3ipp":"$(F_KINOMA)/kinoma-mp3-ipp/kinomamp3ipp.mk",
						"kinomamp4ipp":"$(F_KINOMA)/kinoma-mp4-ipp/kinomamp4ipp.mk",
						"kinomaaacdecipp":null,
						"kinomaavcdecipp":null,
						"kinomamp3decipp":null,
						"kinomamp4decipp":null,
						"kinomapngdecipp":null,
					},
					"build":{
						"SIMULATOR":true,
					},
				},
				"i386":{
					"build":{
						"DEVICE":true,
					},
				},
				"aspen":{
					"build":{
						"DEVICE":true,
					},
					"environment":{
						"KPR_BINARY_NAME":"Kpl",
					},
					"extensions":{
						"FskPinDigitalCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalCreate/FskPinDigitalCreate.mk",
						"FskPinAnalogCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogCreate/FskPinAnalogCreate.mk",
						"FskPinI2CCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CCreate/FskPinI2CCreate.mk",
						"FskPinPWMCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMCreate/FskPinPWMCreate.mk",
						"FskPinSerialCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialCreate/FskPinSerialCreate.mk",
						"FskPinPWMSynthOS":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMSynthOS/FskPinPWMSynthOS.mk",
					},
					"resources":{
						"modules": [
							"$(KPR_HOME)/libraries/HID/HID.mk",
						]
					}
				},
				"poky":{
					"build":{
						"DEVICE":true,
					},
					"environment":{
						"KPR_BINARY_NAME":"Kpl",
					},
					"extensions":{
						"FskPinDigitalCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinDigitalCreate/FskPinDigitalCreate.mk",
						"FskPinAnalogCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinAnalogCreate/FskPinAnalogCreate.mk",
						"FskPinI2CCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinI2CCreate/FskPinI2CCreate.mk",
						"FskPinPWMCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMCreate/FskPinPWMCreate.mk",
						"FskPinSerialCreate":"$(KPR_HOME)/extensions/pins/pins/FskPinSerialCreate/FskPinSerialCreate.mk",
						"FskPinPWMSynthOS":"$(KPR_HOME)/extensions/pins/pins/FskPinPWMSynthOS/FskPinPWMSynthOS.mk",
					},
					"resources":{
						"fsFixes": [
							"$(F_HOME)/build/devices/k4/FSfixes/*"
						],
					}
				},
				"bg3cdp":{
					"build":{
						"DEVICE":true,
					},
				}
			}
		},
		"mac":{
			"build":{
				"SIMULATOR":true,
			},
			"environment":{
				"touchSupported":"true",
			},
			"extensions":{
				"FskCameraAVF":null,
				"kinomaqtdec":"$(F_KINOMA)/kinoma-qt-dec/kinomaqtdec.mk",
				"kprLibrary":null,
			},
		},
		"win":{
			"build":{
				"SIMULATOR":true,
			},
			"environment":{
				"fontsPath":"[applicationPath]fonts/",
			},
			"extensions":{
				"FskCameraDshow":"$(F_MEDIA_READER)/readers/camera-dshow/FskCameraDshow.mk",
				"FskDMOCodec":"$(F_KINOMA)/DMOCodec/FskDMOCodec.mk",
				"FskMediaReaderFLAC":null,
			},
			"resources":{
				"~": [

				]
			},
		},
	},
	"variants":{
		"$(DEVICE)": {
			"environment":{
				"applicationURLs":"file://[applicationPath]apps/",
				"defaultValidationPreferences":"defaults.json",
				"hwpModulePath":"device",
				"shellPath":"[applicationPath]program/deviceShell.xsb",
				"useGL":"0",
			},
			"extensions":{
				"KplFrameBuffer":"$(KPL_HOME)/framebuffer/KplFrameBuffer.mk",
			},
			"resources":{
				"~":[
					"$(CREATE_HOME)/apps/pindetective/simulator"
				],
				".":[

				],
				"modules":[
					"$(KDT_HOME)/kpr/debugger/modules/*",
					"$(CREATE_HOME)/shell/device/*"
				],
				"program":[
					"$(CREATE_HOME)/shell/deviceShell.xml"
				],
			},
		},
		"$(SIMULATOR)": {
			"environment":{
				"applicationURLs":"file://[applicationPath]apps/",
				"defaultValidationPreferences":"defaultsMockup.json",
				"deviceURLs":"file://[applicationPath]devices/",
				"hwpModulePath":"simulator",
				"shellPath":"[applicationPath]program/mockupShell.xsb",
				"useGL":"1",
			},
			"resources":{
				"~":[
					"$(CREATE_HOME)/apps/pindetective/device",
				],
				".":[
					"$(CREATE_HOME)/devices"
				],
				"modules":[
					"$(KDT_HOME)/kpr/simulator/modules/*",
					"$(CREATE_HOME)/shell/simulator/*"
				],
				"program":[
					"$(CREATE_HOME)/shell/mockupShell.xml"
				],
			},
		},
	},
	"instrument":{
		"log":"",
		"syslog":"",
		"trace":false,
		"threads":false,
		"times":false,
		"androidlog":false,
		"kinds":[

		]
	},
}
